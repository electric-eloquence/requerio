{
  "name": "requerio",
  "version": "0.3.1",
  "description": "Redux + jQuery + Cheerio = predictable client-side state + server-side testability",
  "main": "dist/requerio.npm.js",
  "scripts": {
    "bundle": "rollup --config && node run/bundle-client.js",
    "lint": "eslint src && eslint examples",
    "start": "npm run watch",
    "watch": "nodemon --watch src -e js --exec 'npm run bundle'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electric-eloquence/requerio.git"
  },
  "keywords": [
    "cheerio",
    "client",
    "client-side",
    "isomorphic",
    "isomorphism",
    "jquery",
    "redux",
    "server",
    "server-side",
    "side",
    "state",
    "test",
    "testing",
    "unit"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electric-eloquence/requerio/issues"
  },
  "homepage": "https://github.com/electric-eloquence/requerio#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.3",
    "cheerio": "1.0.0-rc.2",
    "eslint": "3.x",
    "nodemon": "^1.18.9",
    "redux": "3.7.2",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "uglify-es": "3.3.9"
  }
}
