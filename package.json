{
  "name": "requerio",
  "version": "0.1.0",
  "description": "Redux + jQuery + Cheerio = predictable client-side state + server-side testability",
  "main": "dist/requerio-node.js",
  "scripts": {
    "bundle": "node run/bundle-node.js && node run/bundle-client.js",
    "lint": "./node_modules/.bin/eslint src/",
    "watch": "./node_modules/.bin/nodemon --watch src -e js --exec 'npm run bundle'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electric-eloquence/requerio.git"
  },
  "keywords": [
    "cheerio",
    "client",
    "client-side",
    "isomorphic",
    "isomorphism",
    "jquery",
    "redux",
    "server",
    "server-side",
    "side",
    "state",
    "test",
    "testing",
    "unit"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/electric-eloquence/requerio/issues"
  },
  "homepage": "https://github.com/electric-eloquence/requerio#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.2",
    "redux": "3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.4.0",
    "eslint": "^3.19.0",
    "nodemon": "^1.12.1",
    "rollup": "^0.50.0",
    "uglify": "^0.1.5"
  }
}
